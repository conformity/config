{
    "name": "conformity/config",
    "description": "A Config Implementation of our config PSR recommendation",
    "homepage": "https://github.com/conformity/config",
    "authors": [
        {
            "name": "leemason",
            "email": "contact@nohalfpixels.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/conformity/config-recommendation"
        }
    ],
    "require": {
        "conformity/config-recommendation" : "dev-master"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "~1.10"
    },
    "autoload": {
        "psr-4": {
            "Conformity\\Config\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Conformity\\Config\\Test\\": "tests"
        }
    }
}
